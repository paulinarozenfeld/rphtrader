<!DOCTYPE html>
<html>
  <head>
    <title>Rphtrader</title>
    <%= csrf_meta_tags %>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Rph Trader</title>

    <%= stylesheet_link_tag    'bootstrap.min.css', media: 'all' %>
    <%= stylesheet_link_tag    'animate.min.css', media: 'all' %>
    <%= stylesheet_link_tag    'font-awesome.min.css', media: 'all' %>
    <%= stylesheet_link_tag    'owl.carousel.min.css', media: 'all' %>
    <%= stylesheet_link_tag    'owl.theme.default.min.css', media: 'all' %>
    <%= stylesheet_link_tag    'settings.css', media: 'all' %>
    <%= stylesheet_link_tag    'offcanvas-menu.css', media: 'all' %>
    <%= stylesheet_link_tag    'style.css', media: 'all' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
